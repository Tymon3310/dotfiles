{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "secondary_prompt": {
    "template": "    ❯ ",
    "foreground": "#ffffff",
    "background": "transparent"
  },
  "transient_prompt": {
    // "template": "   ❯❯ {{ .AbsolutePWD }}\n    ❯ ",
    "template": "╰─❯ ",
    "foreground": "#ffffff",
    "background": "transparent",
    "newline": true
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u256d\u2500",
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ .UserName }}",
          "foreground": "#e6f0ff",
          "background": "#002b54",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\udb85\udc0b",
          "foreground": "#e6f0ff",
          "powerline_symbol": "\ue0b0",
          "background": "#003e7a",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Icon }} ",
          "foreground": "#e6f0ff",
          "powerline_symbol": "\ue0b0",
          "background": "#1e5080",
          "type": "os",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "trailing_diamond": "\ue0b4",
          "template": " \udb80\ude56 {{ path .Path .Location }}",
          "foreground": "#e6f0ff",
          "background": "#005CB3",
          "type": "path",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b6",
          "template": "\udb80\udd09 {{ .CurrentDate | date .Format }} \u2022",
          "foreground": "#e6f0ff",
          "background": "#005CB3",
          "type": "time",
          "style": "diamond"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "style": "austin",
            "threshold": 500
          },
          "trailing_diamond": "\ue0b4",
          "template": " \ueba2 {{ .FormattedMs }}",
          "foreground": "#e6f0ff",
          "background": "#005CB3",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none",
            "display_changing_color": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "full_branch_path": true
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue725 ({{ url .UpstreamIcon .UpstreamURL }} {{ url .HEAD .UpstreamURL }}){{ if gt .Ahead 0 }}<#00aaff> +{{ .Ahead }}</>{{ end }}{{ if gt .Behind 0 }}<#ff4d4d> -{{ .Behind }}</>{{ end }}{{ if .Working.Changed }}<#e6f0ff> \uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }}<#e6f0ff> \uf046 {{ .Staging.String }}</>{{ end }}",
          "foreground": "#e6f0ff",
          "background": "#24292e",
          "type": "git",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718 {{ .Full }}",
          "foreground": "#002b54",
          "background": "#80b2e6",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "display_mode": "files",
            "extensions": [
              "package-lock.json"
            ]
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue71e {{ .Full }}",
          "foreground": "#e6f0ff",
          "background": "#ff4d4d",
          "type": "npm",
          "style": "diamond"
        },
        {
          "properties": {
            "display_mode": "files",
            "extensions": [
              "pnpm-lock.yaml"
            ]
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\udb80\udec1 {{ .Full }}",
          "foreground": "#e6f0ff",
          "background": "#007ACC",
          "type": "pnpm",
          "style": "diamond"
        },
        {
          "properties": {
            "display_mode": "files"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue73c {{ .Full }}",
          "foreground": "#002b54",
          "background": "#80b2e6",
          "type": "python",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue738 {{ .Full }}",
          "foreground": "#e6f0ff",
          "background": "#007ACC",
          "type": "java",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\uf308 {{ .Context }}",
          "foreground": "#e6f0ff",
          "background": "#80b2e6",
          "type": "docker",
          "style": "diamond"
        },
        {
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue73d {{ .Full }}",
          "foreground": "#e6f0ff",
          "background": "#005CB3",
          "type": "php",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "╰─❯",
          "foreground": "#ffffff",
          "type": "text",
          "style": "diamond"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "patch_pwsh_bleed": true,
  "final_space": true
}