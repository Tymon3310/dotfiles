{
    "default_apps": {
        "terminal": "kitty"
    },
    "debug": {
        "try_suppress_errors": true,
        "try_suppress_warnings": false
    },
    "units": {
        "lengths": "meter",
        "weights": "kg",
        "volumes": "l"
    },
    "appearance": {
        "width": 900,
        "height": 593,
        "gsk_renderer": "cairo",
        "icon_paths": ["~/.config/sherlock/icons/"],
        "icon_size": 22,
        "search_icon": true,
        "use_base_css": true
    },
    "behavior": {
        "caching": true,
        "cache": "~/.cache/sherlock_desktop_cache.json",
        "daemonize": false,
        "animation": true
    },
    "binds": {
        "next": "None",
        "prev": "None",
        "modifier": "control"
    },
    "files": {
        "fallback": "~/.config/sherlock/fallback.json",
        "alias": "~/.config/sherlock/sherlock_alias.json",
        "ignore": "~/.config/sherlock/sherlockignore"
    }
}